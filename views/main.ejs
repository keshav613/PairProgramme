  <body>
    <ul id="users"></ul>
    <div class="section-Main">
      <div class="mainWindow">
        <div class="Window">
          <div class="designWindow">
            <div class="sizeArea">
              <label class="designWindow-Label" for="fontSize">Font Size</label>
              <select
                id="fontSize"
                name="fontSize"
                class="input"
                onchange="editorPropertyChanged(null, this.value)"
              >
                <option value="16">16</option>
                <option value="18">18</option>
                <option value="20">20</option>
                <option value="22">22</option>
              </select>
            </div>
            <div class="themeArea">
              <label
                class="designWindow-Label"
                for="theme"
                onchange="editorPropertyChanged(this.value, null)"
                >Theme</label
              >
              <select
                id="theme"
                onchange="editorPropertyChanged(this.value, null)"
                name="theme"
              >
