                <option>mbo</option>
                <option>mdn-like</option>
                <option>midnight</option>
                <option>monokai</option>
                <option>moxer</option>
                <option>neat</option>
                <option>neo</option>
                <option>night</option>
                <option>nord</option>
                <option>oceanic-next</option>
                <option>panda-syntax</option>
                <option>paraiso-dark</option>
                <option>paraiso-light</option>
                <option>pastel-on-dark</option>
                <option>railscasts</option>
                <option>rubyblue</option>
                <option>seti</option>
                <option>shadowfox</option>
                <option>solarized dark</option>
                <option>solarized light</option>
                <option>the-matrix</option>
                <option>tomorrow-night-bright</option>
                <option>tomorrow-night-eighties</option>
                <option>ttcn</option>
                <option>twilight</option>
                <option>vibrant-ink</option>
                <option>xq-dark</option>
                <option>xq-light</option>
                <option>yeti</option>
                <option>yonce</option>
                <option>zenburn</option>
              </select>
            </div>
            <div class="languageArea">
              <label class="designWindow-Label" for="language">language</label>
              <select
                id="language"
                name="language"
                onchange="editorChanged(this.value)"
              >
                <option value="clike">C++</option>
                <option value="python">Python</option>
                <option value="clike">Java</option>
                <option value="javaScript">JavaScript</option>
              </select>
            </div>
          </div>
          <!-- <div class="codeWindow" id="codeMirror">
            <textarea></textarea>
          </div> -->

          <div class="codeWindow" id="codeMirror">
            <!-- <textarea id="codeMirror"></textarea> -->
            <textarea></textarea>
          </div>
          <div class="TestCases">
            <div class="btn btn-AddTestcase">Add Testcase</div>
          </div>

          <div class="IO-container hideElement">
            <div class="run-code">
              <button class="btn-IO btn-IO-runCode btn-IO-default">
                Run Code
              </button>
            </div>
            <div class="IO">
              <button class="btn-IO btn-testcase btn-IO-default">
                Testcase
              </button>
              <button class="btn-IO btn-runCodeResult">Run code result</button>
            </div>
            <div class="IO-textArea-container">
              <textarea class="IO-textArea"></textarea>
            </div>
          </div>

          <!-- <div class=""></div> -->
        </div>

        <!-- </div> -->
        <!-- Window End -->
        <div class="videoAndChatBox">
          <!-- Video Call -->
          <!-- <div id="video-container"> -->
          <div id="video-container">
            <!-- <video autoplay id="second-video"></video>
            <video autoplay id="my-video" muted="true"></video> -->
            <div id="video-grid"></div>
            <!-- <div id="video-grid"></div>
            <div id="video-grid"></div>
            <div id="video-grid"></div>
            <div id="video-grid"></div> -->
          </div>
          <!-- Chat room -->
          <div class="chat-container">
            <div class="panel-body">
              <ul
                class="media-list"
                style="height: 195px; overflow-y: scroll chatbox-listMessages"
              >
                <div id="chatbox-listMessages" style="margin-left: 1rem"></div>
              </ul>
            </div>
            <div class="panel-footer">
              <div class="input-group">
                <input
                  type="text"
                  class="form-control input-chat"
                  placeholder="Enter message"
                  id="userMessage"
                />
                <span class="input-group-btn">
                  <button
                    type="button"
                    class="btn btn-chat"
                    onclick="sendMessage()"
                  >
                    SEND
                  </button>
                </span>
              </div>
            </div>
          </div>
        </div>
        <!-- Video & Chat box end -->
      </div>
      <!-- Main Window END-->
    </div>
